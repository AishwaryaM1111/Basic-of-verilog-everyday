module top_module( 
    input [399:0] a, b,
    input cin,
    output cout,
    output [399:0] sum );
    
    wire [99:0]cwire;
    bcd_fadd instance0(.a(a[3:0]),.b(b[3:0]),.cin(cin),.cout(cwire[0]),.sum(sum[3:0]));
    
    genvar i;
    
    generate
        for (i=1;i<100;i=i+1) begin: dcd_generate_block
            bcd_fadd instancei(.a(a[4*i+3:4*i]),.b(b[4*i+3:4*i]),.cin(cwire[i-1]),.cout(cwire[i]),.sum(sum[4*i+3:4*i]));
        end
    endgenerate
    assign cout = cwire[99];
endmodule
