module top_module( 
    input [99:0] a, b,
    input cin,
    output [99:0] cout,
    output [99:0] sum );
    
    
    fa instance0(a[0],b[0],cin,cout[0],sum[0]);
    
    
    genvar i;
    generate 
        for(i=1;i<100;i=i+1) begin: fall_generate_block
            fa instancei (a[i],b[i],cout[i-1],cout[i],sum[i]);
        end
    endgenerate
    
    

endmodule

module fa(x,y,ci,co,s);
    input x,y,ci;
    output co,s;
    
    assign s=x^y^ci;
    assign co=x&y|y&ci|ci&x;
endmodule

